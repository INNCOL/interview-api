{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH"
            ]
        },
        {
            "src": "/swagger/(.*)",
            "dest": ".well-known/nest-swagger-ui/$1",
            "methods": [
                "GET"
            ],
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate",
                "Content-Type": "text/javascript",
                "Content-Security-Policy": "script-src 'self' 'unsafe-eval';"
            }
        },
        {
            "src": "/_next/(.*)",
            "dest": "_next/$1"
        },
        {
            "src": "/node_modules/(.*)",
            "dest": "node_modules/$1"
        },
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH"
            ]
        }
    ]
}